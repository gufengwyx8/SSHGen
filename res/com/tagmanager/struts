<package name="auditLog" namespace="/auditLog" extends="erp-default">
	<default-action-ref name="index" />
	<action name="listAuditLog" class="auditLogAction" method="listAuditLog">
		<result>/auditLog/listAuditLog.jsp</result>
	</action>
	<action name="saveAuditLog" class="auditLogAction" method="saveAuditLog">
		<result type="stream">
			<param name="contentType">text/html</param>
			<param name="inputName">inputStream</param>
		</result>
	</action>
	<action name="destoryAuditLog" class="auditLogAction" method="destoryAuditLog">
		<result type="stream">
			<param name="contentType">text/html</param>
			<param name="inputName">inputStream</param>
		</result>
	</action>
	<action name="listAuditLogDetail" class="auditLogAction"
		method="listAuditLogDetail">
		<result>/auditLog/listAuditLogDetail.jsp</result>
	</action>
	<action name="submitContent" class="auditLogAction"
		method="submitContent">
		<result type="redirect">/auditLog/listAuditLogDetail?auditLog.id=${auditLog.id}</result>
	</action>
	<action name="download" class="auditLogAction" method="download">
		<result name="success" type="stream">
			<param name="contentType">application/octet-stream;charset=ISO8859-1</param>
			<param name="inputName">inputStream</param>
			<param name="contentDisposition">attachment;filename="${fileFileName}"</param>
			<param name="bufferSize">4096</param>
		</result>
	</action>
</package>

